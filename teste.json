{
    "python.languageServer": "Pylance", // ms-python.vscode-pylance
      "python.testing.unittestEnabled": false, // ms-python.python
      "python.testing.pytestEnabled": true,
      "python.testing.pytestArgs": [], // -x to bail
      "python.linting.flake8Enabled": false,
      "python.linting.pylintArgs": [
        "--load-plugins=pylint_django",
        "--errors-only"
      ],
      "python.formatting.autopep8Args": ["--indent-size=4"],
      //"python.defaultInterpreterPath": "venv/bin/python",
      "[python]": {
        "editor.defaultFormatter": "ms-python.python", // ms-python.python
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
          "source.organizeImports": true
        },
        "editor.formatOnType": true
      },
      "[html]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.html-language-features",
        "editor.quickSuggestions": {
          "other": true,
          "comments": true,
          "strings": true
        }
      },
      "[django-html]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.quickSuggestions": {
          "other": true,
          "comments": true,
          "strings": true
        }
      },
      "files.associations": {
        "*.js": "javascript",
        "*.jsx": "javascriptreact",
        "*.xml": "html",
        "*.svg": "html",
        "*.html": "html",
        "django-html": "html", // batisteo.vscode-django
        "**/*.html": "html",
        "**/templates/**/*.html": "django-html",
        "**/base_templates/**/*.html": "django-html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements"
      },
      "emmet.showSuggestionsAsSnippets": true,
      "emmet.includeLanguages": {
        "django-html": "html", // batisteo.vscode-django
        "javascript": "javascriptreact",
        "typescript": "typescriptreact",
        "postcss": "css",
        "html.erb": "html",
        "erb": "html"
      },
      "beautify.language": {
        "html": [
            "htm",
            "html",
            "django-html"
        ]
    },
      "tabnine.experimentalAutoImports": true,
      "workbench.iconTheme": "material-icon-theme",
      "editor.parameterHints.enabled": false,
      "files.autoSave": "afterDelay",
      "git.enableSmartCommit": true,
      "explorer.compactFolders": false,
      "git.confirmSync": false,
      "mssql.connections": [
        {
          "server": "127.0.0.1",
          "database": "",
          "authenticationType": "SqlLogin",
          "user": "sa",
          "password": "",
          "emptyPasswordInput": false,
          "savePassword": true
        }
      ],
      "workbench.editor.enablePreview": false,
      "[java]": {
        "editor.defaultFormatter": "redhat.java"
      },
      "workbench.colorTheme": "Bearded Theme Arc Blueberry",
      "bracket-pair-colorizer-2.depreciation-notice": false,
      "explorer.confirmDragAndDrop": false,
      "tailwindCSS.emmetCompletions": true,
      "genymotion.genymotion.installedPath": "/home/emerson/genymotion",
      "genymotion.virtualBox.installedPath": "/home/emerson/.config/VirtualBox",
      "json.schemas": [
  
      
      ],
      "typescript.updateImportsOnFileMove.enabled": "always",
      "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
          80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false,
  
        
      },
      "liveServer.settings.donotVerifyTags": true,
      "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
      },
      "markdown.preview.breaks": true,
      "redhat.telemetry.enabled": true,
